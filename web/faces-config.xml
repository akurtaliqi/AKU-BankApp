<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    
    <application>
        <resource-bundle>
            <base-name>ch.hearc.ig.odi.customeraccount.bundle.Resources</base-name>
            <var>resrc></var>
            
        </resource-bundle>
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>fr</supported-locale>
            <supported-locale>en</supported-locale>
        </locale-config>
        
        
        <message-bundle>ch.hearc.ig.odi.customeraccount.bundle.Mes_messages</message-bundle>
    </application>
    
    <navigation-rule>
        <navigation-case>
            <from-action>#{customersList.}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/customersList.xhtml</to-view-id>
        </navigation-case> 
        
    </navigation-rule>

</faces-config>
